<Window x:Class="BatchRename.PrefixSuffixParam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="Edit Parameters" Height="360" Width="400">
    <Canvas>
        <TextBox Name="editTextBox" Width="360" Height="80" Canvas.Left="20" Canvas.Top="89" TextWrapping="WrapWithOverflow"/>
        <Button Content="Submit" IsDefault="True" Width="80" Height="35" Canvas.Left="93" Canvas.Top="196" Click="OnSubmitButtonClick"/>
        <Button Content="Cancel" IsCancel="True" Width="80" Height="35" Canvas.Left="228" Canvas.Top="196" Click="OnCancelButtonClick"/>
    </Canvas>
</Window>
